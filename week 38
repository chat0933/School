class Main
{
	
		public static void main(String args[]){
		Teacher a1 = new Teacher(tmpName, tmpage, tmpisFemale);
		Student b1 = new Student(tmpname, tmpage, tmpisFemale, tmpdata);

		System.out.println(a1);
		System.out.println(b1);
	}
}
	
public class Teacher{
	String name = "Tess";
	String age ="??";
	boolean isFemale = true;

	public Teacher(String tmpname, String tmpage, boolean tmpisFemale)	{
	name=tmpName;
	age=tmpage;
	isFemale=tmpisFemale;
}
}
public class Student
	{
	String Name = "Christian";
	int age = 22;
	boolean isFemale = false;
	String data = "DatamatikerTeamA";


	// constructor;
	Student(String tmpname, int tmpage, boolean tmpisFemale, String tmpdata)	{
		name=tmpname;
		age=tmpage;
		isFemale=tmpisFemale;
		data=tmpdata;
	}
